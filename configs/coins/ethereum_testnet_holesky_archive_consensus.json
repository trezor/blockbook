        "backend_http": 18136,
        "backend_authrpc": 18536,
        "blockbook_internal": 19036,
        "blockbook_public": 19136
    },
    "ipc": {
        "rpc_url_template": "ws://127.0.0.1:{{.Ports.BackendRPC}}",
