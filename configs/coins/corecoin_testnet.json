{
    "coin": {
        "name": "Core Coin Devin",
        "shortcut": "XAB",
        "label": "Core Devin",
        "alias": "corecoin_testnet"
    },
    "ports": {
        "backend_rpc": 8546,
        "backend_message_queue": 0,
        "backend_p2p": 30301,
        "backend_http": 8545,
        "blockbook_internal": 9036,
        "blockbook_public": 9136
    },
    "ipc": {
        "rpc_url_template": "ws://127.0.0.1:{{.Ports.BackendRPC}}",
        "rpc_timeout": 25
    },
    "backend": {
        "package_name": "backend-gocore-devin",
        "package_revision": "a7f8ca53db94bdf57ce9307eedd08bcef5dc07a3",
        "system_user": "corecoin",
        "version": "2.0.6",
        "binary_url": "https://github.com/core-coin/go-core/releases/download/v2.0.6/gocore-linux-x86_64",
        "verification_type": "sha256",
        "verification_source": "f9aedc9f050e78e34c2f1667f41956d398bd965fe97717296805409b3fa3eb80",
        "extract_command": "mv ${ARCHIVE} backend/gocore && chmod +x backend/gocore && echo",
        "exclude_files": [],
        "exec_command_template": "/bin/sh -c '{{.Env.BackendInstallPath}}/{{.Coin.Alias}}/gocore --syncmode full --devin --txlookuplimit 0 --ipcdisable --cache 1024 --nat none --datadir {{.Env.BackendDataPath}}/{{.Coin.Alias}}/backend --port {{.Ports.BackendP2P}} --ws --ws.addr 127.0.0.1 --ws.port {{.Ports.BackendRPC}} --ws.origins \"*\" --ws.api \"xcb,net,web3,debug,txpool\" --http --http.port {{.Ports.BackendHttp}} --http.addr 127.0.0.1 --http.corsdomain \"*\" --http.vhosts \"*\" --http.api \"xcb,net,web3,debug,txpool\" 2>>{{.Env.BackendDataPath}}/{{.Coin.Alias}}/backend/{{.Coin.Alias}}.log'",
        "logrotate_files_template": "{{.Env.BackendDataPath}}/{{.Coin.Alias}}/backend/{{.Coin.Alias}}.log",
        "postinst_script_template": "",
        "service_type": "simple",
        "service_additional_params_template": "",
        "protect_memory": true,
        "mainnet": false,
        "server_config_file": "",
        "client_config_file": "",
        "platforms": {
            "arm64": {
                "binary_url": "https://github.com/core-coin/go-core/releases/download/v2.0.6/gocore-linux-arm64",
                "verification_source": "cd79887b116b5e9822bec63dcc553d646234f1e76be24a43285821881684ff5a"
            }
        }
    },
    "blockbook": {
        "package_name": "blockbook-corecoin",
        "system_user": "blockbook-corecoin",
        "internal_binding_template": ":{{.Ports.BlockbookInternal}}",
        "public_binding_template": ":{{.Ports.BlockbookPublic}}",
        "explorer_url": "",
        "additional_params": "",
        "block_chain": {
            "parse": true,
            "mempool_workers": 8,
            "mempool_sub_workers": 2,
            "block_addresses_to_keep": 300,
            "additional_params": {
                "mempoolTxTimeoutHours": 48,
                "queryBackendOnMempoolResync": false
            }
        }
    },
    "meta": {
        "package_maintainer": "IT",
        "package_maintainer_email": "it@satoshilabs.com"
    }
}
