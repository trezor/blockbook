{
    "coin_name": "Satoxcoin",
    "coin": {
        "name": "Satoxcoin",
        "shortcut": "SATOX",
        "label": "Satoxcoin",
        "alias": "satoxcoin"
    },
    "ports": {
        "backend_rpc": 7777,
        "backend_message_queue": 38777,
        "blockbook_internal": 9077,
        "blockbook_public": 9177
    },
    "ipc": {
        "rpc_url_template": "http://127.0.0.1:{{.Ports.BackendRPC}}",
        "rpc_user": "your_rpc_username",
        "rpc_pass": "your_rpc_password",
        "rpc_timeout": 25,
        "message_queue_binding_template": "tcp://127.0.0.1:{{.Ports.BackendMessageQueue}}"
    },
    "backend": {
        "package_name": "backend-satoxcoin",
        "package_revision": "satoshilabs-1",
        "system_user": "satoxcoin",
        "version": "latest",
        "binary_url": "",
        "verification_type": "sha256",
        "verification_source": "",
        "extract_command": "tar -C backend --strip 1 -xf",
        "exclude_files": [
            "bin/satoxcoin-qt"
        ],
        "exec_command_template": "{{.Env.BackendInstallPath}}/{{.Coin.Alias}}/bin/satoxcoind -datadir={{.Env.BackendDataPath}}/{{.Coin.Alias}}/backend -conf={{.Env.BackendInstallPath}}/{{.Coin.Alias}}/{{.Coin.Alias}}.conf -pid=/run/{{.Coin.Alias}}/{{.Coin.Alias}}.pid",
        "logrotate_files_template": "{{.Env.BackendDataPath}}/{{.Coin.Alias}}/backend/*.log",
        "postinst_script_template": "",
        "service_type": "forking",
        "service_additional_params_template": "",
        "protect_memory": true,
        "mainnet": true,
        "server_config_file": "satoxcoin.conf",
        "client_config_file": "satoxcoin_client.conf",
        "additional_params": {
            "deprecatedrpc": "estimatefee"
        }
    },
    "blockbook": {
        "package_name": "blockbook-satoxcoin",
        "system_user": "blockbook-satoxcoin",
        "internal_binding_template": ":{{.Ports.BlockbookInternal}}",
        "public_binding_template": ":{{.Ports.BlockbookPublic}}",
        "explorer_url": "",
        "additional_params": "",
        "block_chain": {
            "parse": true,
            "mempool_workers": 8,
            "mempool_sub_workers": 2,
            "block_addresses_to_keep": 300,
            "xpub_magic": 48882142,
            "slip44": 9007,
            "additional_params": {
                "fiat_rates": "coingecko",
                "fiat_rates_vs_currencies": "USD,BTC,ETH",
                "fiat_rates_params": "{\"coin\": \"satoxcoin\", \"periodSeconds\": 600}"
            }
        }
    },
    "meta": {
        "package_maintainer": "Satoxcoin Core Developer"
    }
} 