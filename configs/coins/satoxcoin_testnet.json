{
    "coin": {
        "name": "Satoxcoin Testnet",
        "shortcut": "SATOX",
        "label": "Satoxcoin Testnet",
        "alias": "satoxcoin_testnet"
    },
    "ports": {
        "backend_rpc": 19766,
        "backend_message_queue": 38966,
        "blockbook_internal": 6111,
        "blockbook_public": 6110
    },
    "ipc": {
        "rpc_url_template": "http://127.0.0.1:{{.Ports.BackendRPC}}",
        "rpc_user": "your_rpc_username",
        "rpc_pass": "your_rpc_password",
        "rpc_timeout": 25,
        "message_queue_binding_template": "tcp://127.0.0.1:{{.Ports.BackendMessageQueue}}"
    },
    "backend": {
        "package_name": "backend-satoxcoin-testnet",
        "package_revision": "satoshilabs-1",
        "system_user": "satoxcoin-testnet",
        "version": "latest",
        "binary_url": "https://github.com/satoverse/satoxcoin/releases/latest/download/satoxcoin-latest-x86_64-linux-gnu.tar.gz",
        "verification_type": "sha256",
        "verification_source": "https://github.com/satoverse/satoxcoin/releases/latest/download/SHA256SUMS",
        "extract_command": "tar -C backend --strip 1 -xf",
        "exclude_files": ["bin/satoxcoin-qt"],
        "exec_command_template": "{{.Env.BackendInstallPath}}/{{.Coin.Alias}}/bin/satoxcoind -testnet -datadir={{.Env.BackendDataPath}}/{{.Coin.Alias}}/backend -conf={{.Env.BackendInstallPath}}/{{.Coin.Alias}}/{{.Coin.Alias}}.conf -pid=/run/{{.Coin.Alias}}/{{.Coin.Alias}}.pid",
        "logrotate_files_template": "{{.Env.BackendDataPath}}/{{.Coin.Alias}}/backend/*.log",
        "postinst_script_template": "",
        "service_type": "forking",
        "service_additional_params_template": "",
        "protect_memory": true,
        "mainnet": false,
        "server_config_file": "bitcoin_like.conf",
        "client_config_file": "bitcoin_like_client.conf",
        "additional_params": {
            "deprecatedrpc": "estimatefee"
        }
    },
    "blockbook": {
        "package_name": "blockbook-satoxcoin-testnet",
        "system_user": "blockbook-satoxcoin-testnet",
        "internal_binding_template": ":{{.Ports.BlockbookInternal}}",
        "public_binding_template": ":{{.Ports.BlockbookPublic}}",
        "explorer_url": "",
        "additional_params": "-dbcache=1073741824 -enablesubnewtx -extendedindex",
        "block_chain": {
            "parse": true,
            "subversion": "/Satoxcoin Core:latest/",
            "mempool_workers": 8,
            "mempool_sub_workers": 2,
            "block_addresses_to_keep": 300,
            "xpub_magic": 70615956,
            "xpub_magic_segwit_p2sh": 71979636,
            "xpub_magic_segwit_native": 73342196,
            "slip44": 9007,
            "additional_params": {
                "fiat_rates": "coingecko",
                "fiat_rates_vs_currencies": "AED,ARS,AUD,BDT,BHD,BMD,BRL,CAD,CHF,CLP,CNY,CZK,DKK,EUR,GBP,HKD,HUF,IDR,ILS,INR,JPY,KRW,KWD,LKR,MMK,MXN,MYR,NGN,NOK,NZD,PHP,PKR,PLN,RUB,SAR,SEK,SGD,THB,TRY,TWD,UAH,USD,VEF,VND,ZAR,BTC,ETH",
                "fiat_rates_params": "{\"coin\": \"satoxcoin\", \"periodSeconds\": 300}"
            }
        }
    },
    "meta": {
        "package_maintainer": "IT",
        "package_maintainer_email": "it@satoshilabs.com"
    }
} 