        "backend_http": 18126,
        "backend_authrpc": 18526,
        "blockbook_internal": 19026,
        "blockbook_public": 19126
    },
    "ipc": {
        "rpc_url_template": "ws://127.0.0.1:{{.Ports.BackendRPC}}",
