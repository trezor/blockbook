{
    "coin": {
        "name": "Ethereum Testnet Goerli",
        "shortcut": "tGOR",
        "label": "Ethereum Goerli",
        "alias": "ethereum_testnet_goerli"
    },
    "ports": {
        "backend_rpc": 18026,
        "backend_message_queue": 0,
